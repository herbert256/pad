{sequence '3..5', name='abc'}

{if 4 in abc }  ok {else} nok {/if}
{if 6 in abc } nok {else}  ok {/if}

{if 3 eq action:count(abc)}  ok {else} nok {/if}
{if action:count(abc) eq 4} nok {else}  ok {/if}

{if 3 in action:count(abc)}  ok {else} nok {/if}
{if 4 in action:count(abc)} nok {else}  ok {/if}

{sequence '8..10', name='q1'}
{sequence '1..10', name='q2'}

{if q1 eq action:last(q2,3)}  ok {else} nok {/if}
{if q1 ne action:last(q2,3)} nok {else}  ok {/if}
