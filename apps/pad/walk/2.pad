{table}

{demo}

{myTable}

  {set $loop = 1}

  {while $loop LE 5}

    <tr>
      <td> {first}  X {/first}  </td>
      <td> {middle} X {/middle} </td>
      <td> {last}   X {/last}   </td>
      <td> {border} X {/border} </td>
    </tr>

    {increment $loop}

  {/while}

{/myTable}

{/demo}

{/table}